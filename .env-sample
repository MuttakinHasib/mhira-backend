APP_NAME=mhira
APP_ENV=local
APP_DEBUG=true

SERVER_PORT = 3000
SECRET=
REFRESH_TOKEN_SECRET=
TOKEN_LIFE=15763000
REFRESH_TOKEN_LIFE=31536000

# Intial password for super admin user. Default=`superadmin` if not defined
SUPERADMIN_PASSWORD=

# https://github.com/typeorm/typeorm/blob/master/docs/using-ormconfig.md

TYPEORM_CONNECTION = postgres
TYPEORM_HOST = localhost
TYPEORM_USERNAME = postgres
TYPEORM_PASSWORD = postgres
TYPEORM_DATABASE = mhira
TYPEORM_PORT = 5432
TYPEORM_SYNCHRONIZE = true
TYPEORM_LOGGING = true
TYPEORM_ENTITIES_DIR = /../**/*.model.{js,ts}
TYPEORM_MIGRATIONS_TABLE_NAME = migrations
TYPEORM_MIGRATIONS = migration/*.js
TYPEORM_MIGRATIONS_DIR = src/migrations

# Valid values; 'memory', 'redis', 'mongo'
CACHE_STORE=mongo

MONGODB_CONNECTION_STRING = mongodb://localhost:27017/questionnaireDB

# Flag to enable or disable Graphql Playground. Disabled by default
GRAPHQL_PLAYGROUND_ENABLED=false